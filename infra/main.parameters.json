{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environment": {
      "value": "prod"
    },
    "location": {
      "value": "australiaeast"
    },
    "hcaptchaSecret": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/{rg-name}/providers/Microsoft.KeyVault/vaults/{kv-name}"
        },
        "secretName": "hcaptcha-secret"
      }
    },
    "csrfSecret": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/{rg-name}/providers/Microsoft.KeyVault/vaults/{kv-name}"
        },
        "secretName": "csrf-secret"
      }
    },
    "tenantId": {
      "value": "your-tenant-id"
    },
    "clientId": {
      "value": "your-client-id"
    },
    "clientSecret": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/{rg-name}/providers/Microsoft.KeyVault/vaults/{kv-name}"
        },
        "secretName": "graph-client-secret"
      }
    },
    "recipientEmail": {
      "value": "contact@connectedsystems.com"
    },
    "senderEmail": {
      "value": "noreply@connectedsystems.com"
    },
    "corsOrigin": {
      "value": "https://connectedsystems.com"
    }
  }
}
